# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /                           controllers.Application.index()

POST	/Recommandations/	controllers.Application.recommandations()

GET		/Recommand/:userId/:time/:longitude/:latitude	controllers.Application.getRecommandations(userId : Integer, time : Long, longitude : String, latitude : String)

#GET		/Try/:userId/:time/:longitude/:latitude	controllers.Application.recommandations(userId : Integer, time : Long, longitude : String, latitude : String, types="")
