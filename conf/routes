# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /	controllers.Application.index()

GET		/Recommand/:userId/:time/:longitude/:latitude	controllers.Application.getRecommandations(userId : String, time : Long, longitude : String, latitude : String)

GET 	/Similarity controllers.Application.similarity()

POST	/Recommandations	controllers.Application.recommandations()

POST 	/Emotions	controllers.Application.emotions()

POST	/Login	controllers.Application.login()

POST	/Checkins	controllers.Application.saveCheckins()